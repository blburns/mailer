# Daemon environment configuration
# Place at /etc/dreamlikelabs/${APP_NAME}/env.conf

APP_NAME=mailer
APP_HOME=/opt/dreamlikelabs/${APP_NAME}
VENV=/opt/dreamlikelabs/${APP_NAME}/venv
USER=www-data
GROUP=www-data
PORT=5025
THREADS=4
TIMEOUT=60
PIDFILE=/var/run/dreamlikelabs/${APP_NAME}.pid
LOGFILE=/var/log/dreamlikelabs/${APP_NAME}.log
FLASK_ENV=production
FLASK_DEBUG=False


