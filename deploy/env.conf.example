# Daemon environment configuration
# Place at /etc/dreamlikelabs/mailer/env.conf

APP_HOME=/opt/dreamlikelabs/mailer
VENV=/opt/dreamlikelabs/mailer/venv
USER=www-data
GROUP=www-data
PORT=5025
THREADS=4
TIMEOUT=60
PIDFILE=/var/run/dreamlikelabs/mailer.pid
LOGFILE=/var/log/dreamlikelabs/mailer.log
FLASK_ENV=production
FLASK_DEBUG=False


